{
  "version": "0.0.0",
  "devDependencies": {
    "@types/jest": "24.0.21",
    "inquirer": "7.0.0",
    "jasmine-check": "1.0.0-rc.0",
    "jest": "24.9.0",
    "lerna": "3.18.3",
    "prettier": "1.18.2",
    "chalk": "2.4.2",
    "yeoman-generator": "4.2.0",
    "yosay": "2.0.2",
    "rimraf": "3.0.0",
    "testcheck": "1.0.0-rc.2",
    "tslint": "5.20.0",
    "typescript": "3.6.4",
    "yo": "3.1.0"
  },
  "manager": "yarn",
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "resolutions": {
    "deep-extend": ">=0.5.1",
    "mem": ">=4.0.0"
  },
  "scripts": {
    "build": "yarn build:expect-more && yarn build:expect-more-jest",
    "build:expect-more": "tsc --project ./packages/expect-more",
    "build:expect-more-jest": "tsc --project ./packages/expect-more-jest",
    "format": "prettier --write '**/*.md' './*.{js,json}' './scripts/**/*.js' './packages/*/src/**/*.ts' './packages/*/test/**/*.ts'",
    "generate:assertion": "yo expect-more:assertion",
    "generate:matcher": "yo expect-more:matcher",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prebuild": "rimraf ./packages/*/dist",
    "prelint": "yarn format",
    "test": "jest"
  },
  "workspaces": [
    "packages/*",
    "scripts/*"
  ]
}
