{
  "version": "0.0.0",
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "jasmine-check": "1.0.0-rc.0",
    "jest": "25.2.3",
    "lerna": "3.20.2",
    "prettier": "2.0.2",
    "rimraf": "3.0.2",
    "testcheck": "1.0.0-rc.2",
    "tslint": "6.1.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "manager": "yarn",
  "private": true,
  "resolutions": {
    "deep-extend": ">=0.5.1",
    "mem": ">=4.0.0"
  },
  "scripts": {
    "build": "yarn build:expect-more && yarn build:expect-more-jest",
    "build:expect-more": "tsc --project ./packages/expect-more",
    "build:expect-more-jest": "tsc --project ./packages/expect-more-jest",
    "generate-matchers": "rm ./packages/expect-more-jest/src/*.ts ./packages/expect-more-jest/test/* && node ./scripts/generate-matchers.js && yarn lint",
    "lint": "eslint --fix --ext .ts .",
    "prebuild": "rimraf ./packages/*/dist",
    "test": "jest"
  },
  "workspaces": [
    "packages/*",
    "scripts/*"
  ]
}
