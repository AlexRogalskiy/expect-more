{
  "compilerOptions": {
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6"],
    "noEmitHelpers": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "stripInternal": true,
    "target": "es3",
    "types": ["jest"]
  }
}
